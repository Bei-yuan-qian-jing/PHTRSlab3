<p id="notice"><%= notice %></p>

<h1>Workorders</h1>

<table>
  <thead>
    <tr>
      <th>Location</th>
      <th>Size</th>
      <th>Crew id</th>
      <th>Peoplenumber</th>
      <th>Assign equipment</th>
      <th>Time</th>
      <th>Status</th>
      <th>Material used</th>
      <th>Cost</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @workorders.each do |workorder| %>
      <tr>
        <td><%= workorder.location %></td>
        <td><%= workorder.size %></td>
        <td><%= workorder.crew_ID %></td>
        <td><%= workorder.peoplenumber %></td>
        <td><%= workorder.assign_equipment %></td>
        <td><%= workorder.time %></td>
        <td><%= workorder.status %></td>
        <td><%= workorder.material_used %></td>
        <td><%= workorder.cost %></td>
        <td><%= link_to 'Show', workorder %></td>
        <td><%= link_to 'Edit', edit_workorder_path(workorder) %></td>
        <td><%= link_to 'Destroy', workorder, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Workorder', new_workorder_path %>
